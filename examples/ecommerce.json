{
  "server": {
    "port": 3000,
    "cors": true,
    "logging": true,
    "delay": 100
  },
  "endpoints": [
    {
      "path": "/api/products",
      "method": "GET",
      "response": {
        "status": 200,
        "data": [
          { "id": 1, "name": "T-Shirt", "price": 19.99, "category": "clothing" },
          { "id": 2, "name": "Jeans", "price": 49.99, "category": "clothing" },
          { "id": 3, "name": "Sneakers", "price": 79.99, "category": "shoes" }
        ]
      }
    },
    {
      "path": "/api/cart",
      "method": "POST",
      "response": {
        "status": 201,
        "data": { "message": "Item added to cart", "cartId": "cart_123" }
      }
    },
    {
      "path": "/api/orders",
      "method": "POST",
      "delay": 2000,
      "response": {
        "status": 201,
        "data": { "orderId": "order_456", "status": "processing" }
      }
    }
  ]
}