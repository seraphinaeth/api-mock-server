{
  "server": {
    "port": 3000,
    "cors": true,
    "logging": true,
    "delay": 0
  },
  "endpoints": [
    {
      "path": "/api/feed",
      "method": "GET",
      "response": {
        "status": 200,
        "data": [
          { "id": 1, "author": "john_doe", "content": "Just had a great coffee!", "likes": 15 },
          { "id": 2, "author": "jane_smith", "content": "Working on a new project", "likes": 8 }
        ]
      }
    },
    {
      "path": "/api/posts",
      "method": "POST",
      "response": {
        "status": 201,
        "data": { "id": 3, "message": "Post created successfully" }
      }
    },
    {
      "path": "/api/likes/:postId",
      "method": "POST",
      "response": {
        "status": 200,
        "data": { "message": "Post liked", "totalLikes": 16 }
      }
    }
  ]
}